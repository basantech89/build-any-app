{
	"name": "build-any-app",
	"version": "2.0.2",
	"description": "Create your application",
	"license": "Apache License 2.0",
	"author": "Basant Soni",
	"main": "dist/index.js",
	"bin": {
		"build-any-app": "dist/index.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "cross-env NODE_ENV=development ts-node --experimental-specifier-resolution node -T src/index.ts",
		"prebuild": "rimraf dist",
		"build": "babel src --extensions .js,.ts --out-dir dist",
		"postbuild": "ts-node --experimental-specifier-resolution node -T scripts/postbuild.ts",
		"lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
		"prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|md)\"",
		"format": "npm run prettier -- --write",
		"check-format": "npm run prettier -- --list-different",
		"check-types": "tsc",
		"test": "is-ci-cli test:coverage test:watch",
		"test:coverage": "jest --coverage",
		"test:watch": "jest --watch",
		"test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
		"validate": "npm-run-all --parallel test:coverage check-types check-format lint build",
		"setup": "npm install && npm run validate",
		"prepare": "husky install",
		"commit": "git-cz",
		"semantic-release": "semantic-release"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-git"
		}
	},
	"dependencies": {
		"@babel/core": "^7.20.7",
		"chalk": "^4.1.2",
		"cosmiconfig": "^8.0.0",
		"enquirer": "^2.3.6",
		"execa": "5.1.1",
		"fs-extra": "^10.1.0",
		"libsodium-wrappers": "^0.7.10",
		"node-fetch": "2",
		"prettier": "^2.8.1",
		"prettier-plugin-sh": "^0.12.8",
		"simple-git": "^3.15.1",
		"which": "^2.0.2",
		"winston": "^3.8.1",
		"yargs": "^17.5.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.18.10",
		"@babel/preset-env": "^7.18.10",
		"@babel/preset-typescript": "^7.18.6",
		"@babel/register": "^7.18.9",
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.3.0",
		"@jest/globals": "^29.1.2",
		"@jest/types": "^29.1.2",
		"@semantic-release/changelog": "^6.0.1",
		"@semantic-release/git": "^10.0.1",
		"@types/fs-extra": "^9.0.13",
		"@types/libsodium-wrappers": "^0.7.10",
		"@types/node": "^18.7.11",
		"@types/node-fetch": "^2.6.2",
		"@types/which": "^2.0.1",
		"@types/yargs": "^17.0.11",
		"@typescript-eslint/eslint-plugin": "^5.34.0",
		"@typescript-eslint/parser": "^5.34.0",
		"all-contributors-cli": "^6.24.0",
		"babel-jest": "^29.1.2",
		"babel-loader": "^9.1.0",
		"babel-plugin-minify-mangle-names": "^0.5.1",
		"babel-plugin-module-resolver": "^4.1.0",
		"babel-preset-minify": "^0.5.2",
		"conventional-changelog-conventionalcommits": "^5.0.0",
		"cross-env": "^7.0.3",
		"cz-git": "^1.3.12",
		"eslint": "8.22.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"husky": "^8.0.0",
		"is-ci-cli": "^2.2.0",
		"jest": "^29.1.2",
		"lint-staged": "^13.0.3",
		"npm-run-all": "^4.1.5",
		"p-locate": "^6.0.0",
		"rimraf": "^3.0.2",
		"semantic-release": "^19.0.5",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.0",
		"typescript": "^4.7.4"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"web application",
		"react",
		"build app",
		"build",
		"make",
		"make app",
		"eslint",
		"prettier",
		"jest"
	]
}
